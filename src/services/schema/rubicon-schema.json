{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "allowedSlippage": { "type": "string" },
    "gasLimitEstimate": { "type": "integer" },
    "ttl": { "type": "integer" },
    "privateKeys": {
      "type": "object",
      "patternProperties": {
        "^0x\\w{0,40}$": { "type": "string" }
      },
      "additionalProperties": false
    }
  },
  "additionalProperties": false,
  "required": ["allowedSlippage", "gasLimitEstimate", "ttl", "privateKeys"]
}
